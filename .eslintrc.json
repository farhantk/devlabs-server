{
    "env": {
        "node": true,
        "commonjs": true,
        "es2021": true,
        "jest": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-underscore-dangle": "off",
        "class-methods-use-this": "off",
        "no-unused-vars": "warn",
        "no-shadow": "off",
        "camelcase": "off",
        "max-len": [
            "error",
            {
                "code": 150
            }
        ],
        "object-curly-newline": "off",
        "no-param-reassign": "off",
        "consistent-return": "off",
        "no-await-in-loop": "off"
    }
}